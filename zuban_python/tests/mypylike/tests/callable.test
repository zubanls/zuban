S = TypeVar('S')
T = TypeVar('T')
U = TypeVar('U')

def comp(g: Callable[[T], U], f: Callable[[S], T]) -> Callable[[S], U]:
    def composed(x: S) -> U:
        return g(f(x))
    return composed

def test(s: str) -> int:
